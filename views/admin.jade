extends layout

block content
    div.navbar.navbar-inverse.navbar-fixed-top(role='navigation', ng-app='admin')
        div.container-fluid
            div.navbar-header
                button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
                    span.sr-only Toggle Navigation
                    span.icon-bar
                    span.icon-bar
                    span.icon-bar
                a.navbar-brand(href='#') Wei To Ticket
            div.navbar-collapse.collapse
                ul.nav.navbar-nav.navbar-right
                    li
                        a(href='#') Dashboard
                    li
                        a(href='#') Settings
                    li
                        a(href='#') Profile
                    li
                        a(href='#') Help

    div.container-fluid
        div.row
            div.col-sm-3.col-md-2.sidebar.t-spaceIcons
                h3 Overview
                ul.nav.nav-sidebar
                    li.active
                        a(href='#')
                            span.glyphicon.glyphicon-cloud
                            | Welcome
                    li
                        a(href='#')
                            span.glyphicon.glyphicon-th-large
                            | View Tickets
                    li
                        a(href='#')
                            span.glyphicon.glyphicon-plus
                            | New Ticket
                    li
                        a(href='#')
                            span.glyphicon.glyphicon-search
                            | Search Ticket
                h3 Control Panel
                ul.nav.nav-sidebar
                    li
                        a(href='')
                            span.glyphicon.glyphicon-user
                            | View Users
                    li
                        a(href='')
                            span.glyphicon.glyphicon-certificate
                            | New User
                    li
                        a(href='')
                            span.glyphicon.glyphicon-envelope
                            | Mail Settings
                    li
                        a(href='')
                            span.glyphicon.glyphicon-cog
                            | Other Settings
                hr
                div.t-bottom
                    p.text-center Created By The Wei Team
            div.col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-2.main(ui-view)